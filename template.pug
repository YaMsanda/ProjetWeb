doctype html
html
    include includes/head.pug
    case activePage
            when 'search'
                include assets/css/searchStyle.pug
            when 'accueil'
                include assets/css/accueilStyle.pug
            when 'signin'
                include assets/css/signinStyle.pug
            when 'signedin'
                include assets/css/signedinStyle.pug
            when 'addbien'
                include assets/css/addbienStyle.pug
            when 'contact'
                include assets/css/contactStyle.pug
            default
                include assets/css/accueilStyle.pug
    include includes/header.pug
    
    main
        case activePage
            when 'search'
                include includes/search.pug
            when 'accueil'
                include includes/accueil.pug
            when 'signin'
                include includes/signin.pug
            when 'signedin'
                include includes/signedin.pug
            when 'addbien'
                include includes/addbien.pug
            when 'contact'
                include includes/contact.pug
            default
                include includes/accueil.pug